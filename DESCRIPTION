Package: multigroup.vaccine
Type: Package
Title: Multigroup Vaccine Model
Version: 0.1.1
Authors@R:
    c(person("Damon", "Toth", , "damon.toth@hsc.utah.edu", role = c("aut", "ctb"),
           comment = c(ORCID = "0000-0001-7393-4814")),
